{
  "name": "extension",
  "version": "1.0.0",
  "description": "Optract background script",
  "main": "background.js",
  "scripts": {
    "clear": "rm -vf dist/chrome/*.html dist/chrome/*.js dist/chrome/*.css dist/chrome/*.json",
    "build": "webpack",
    "install_css": "cp -v css/*.css dist/chrome/",
    "install_html": "cp -v html/*.html dist/chrome/",
    "install_js": "cp -v scripts/content.js scripts/influenced.js scripts/sorry.js dist/chrome/",
    "install_ui": "cd UI && npm run distRelease",
    "install_mf": "cp -v manifest.json dist/chrome/",
    "release": "npm run clear && npm run build && npm run install_html && npm run install_css && npm run install_js && npm run install_mf && npm run install_ui"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "async": "^2.6.2",
    "rpc-websockets": "^4.3.5",
    "secure-random-password": "^0.2.1",
    "web-ext": "^3.2.0",
    "webpack": "^4.41.0"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.9"
  }
}
